{
  "name": "stream2own",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "lerna run build --scope beta-player --stream",
    "build:embed": "lerna run build --scope embed --stream",
    "dev": "lerna run --scope beta-player dev --parallel --",
    "publish": "lerna publish from-package",
    "start": "lerna run --scope beta-player start --parallel",
    "start:embed": "lerna run --scope embed start --parallel",
